.scroll {
  width: 100%;
  height: 100rpx;
  display: flex; 
  align-items: center;
  overflow-x: auto;
  white-space: nowrap;

  margin-left: -13%;
}
.scroll .item {
  width: 40rpx;
  padding: 0 36rpx;
  height: 100rpx;
  line-height: 100rpx;
  display: inline-block;
  position: relative;
  color: white;
  
}
.scroll .item .line {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  margin: 0 auto;
  margin-left: 53rpx;
  width: 15rpx;
  height: 15rpx;
  border-radius: 100%;
  background: transparent;
  color: white;
}
.scroll .item.active {
  color: #e6ae5d;
  animation-name: zoom;
  transform-origin: center;
  animation: zoom 0.4s;
  -webkit-animation: zoom 0.4s;
}
.scroll .item.active .line {
  background: #e6ae5d;
  border-radius: 10rpx;
}
