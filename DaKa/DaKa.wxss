.rotateIn {
  animation-name: rotateIn;
  transform-origin: center;
  animation: rotateIn 1s;
  -webkit-animation: rotateIn 1s;
}
@keyframes rotateIn {
  from {
    transform: rotate3d(0, 0, 1, -200deg);
    opacity: 0;
  }
  to {
    transform: translate3d(0, 0, 0);
    opacity: 1;
  }
}


.animated {
  animation-duration: 0.4s;
  animation-fill-mode: both;
}

.zoom {
  animation-name: zoom;
  transform-origin: center;
  animation: zoom 0.4s;
  -webkit-animation: zoom 0.4s; 
}

@keyframes zoom {
  from {
    transform: scale(1); /*开始为原始大小*/
  }
  25% {
    transform: scale(0.9); 
  }
  50% {
    transform: scale(1.0);
  }
  75% {
    transform: scale(1.1);
  }
}

.flipInX {
  backface-visibility: visible !important;
  animation-name: flipInX;
  animation: flipInX 1s;
  -webkit-animation: flipInX 1s;
}

@keyframes flipInX {
  from {
    transform: perspective(400rpx) rotate3d(1, 0, 0, 90deg);
    animation-timing-function: ease-in;
    opacity: 0;
  }
  40% {
    transform: perspective(400rpx) rotate3d(1, 0, 0, -20deg);
    animation-timing-function: ease-in;
  }
  60% {
    transform: perspective(400rpx) rotate3d(1, 0, 0, 10deg);
    opacity: 1;
  }
  80% {
    transform: perspective(400rpx) rotate3d(1, 0, 0, -5deg);
  }
  to {
    transform: perspective(400rpx);
  }
}
